{
  "name": "blobfolio\/blob-mimes",
  "description": "A comprehensive MIME and file extension tool for PHP.",
  "keywords": [
    "mimes",
    "files",
    "extensions",
    "suffix"
  ],
  "version": "8.0.0",
  "type": "library",
  "homepage": "https:\/\/github.com\/Blobfolio\/blob-mimes",
  "license": "WTFPL",
  "authors": [
    {
      "name": "Josh Stoik",
      "email": "josh@blobfolio.com",
      "homepage": "https:\/\/blobfolio.com"
    }
  ],
  "require": {
    "php": ">= 7.0",
    "ext-filter": "*",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-bcmath": "*",
    "ext-hash": "*",

    "blobfolio\/blob-common": "*"
  },
  "require-dev": {
    "phpunit\/phpunit": "5.7.*"
  },
  "config": {
    "vendor-dir": "lib\/vendor",
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "blobfolio\\mimes\\":"lib\/blobfolio\/mimes"
    }
  }
}
